<main class="view-menu">
   <h1>Menu</h1>

   <section class="table-container">
      <table class="table-stripped">
         <thead class="table-title">
            <tr>
               <th><p>id</p></th>
               <th><p>section</p></th>
               <th><p>items</p></th>
               <th><p>description</p></th>
               <th><p>price</p></th>
               <th><p>icons</p></th>
            </tr>
         </thead>

         {{#each menu}}
         <tbody class="table-body">
            <tr>
               <td><p>{{id}}</p></td>
               <td><p>{{section}}</p></td>
               <td><p>{{item}}</p></td>
               <td><p>{{descrip}}</p></td>
               <td><p>{{price}}</p></td>
               <td>
                  <a href=""><i class="far fa-edit fa-1x"></i></a>
                  <a href=""><i class="fas fa-trash fa-1x delete-item"></i></a>
               </td>
            </tr>
         </tbody>
         {{/each}}
      </table>
   </section>
</main>